<view class="body">
<!-- 分隔 -->
  <view wx:if="{{orderType == 3}}"><image style="width:750rpx;height:119rpx;" src="data:image/png;base64,"></image></view>
  
  <view class="dividing_line"><text>\n</text></view>
  <view class='header-title'>
    <view class="product-price-v">
      <text>购物清单</text>
    </view> 
    <view class="ensure">
      <block><image src="{{picUrl}}?x-oss-process=image/resize,w_80/auto-orient,1/quality,q_95/format,jpg" style="width:156rpx;height:156rpx;"></image></block>
      <view class="product-text">
        <view>{{name}}</view>
        <view class="guige">规格：标准</view>
      </view>
       <view class="price-text">
        <view style="color:#f66741;">{{orderPrice}}元</view>
        <view class="guige">×1</view>
      </view>
    </view>
  </view>
  
  <!-- 分隔 -->
  <view class="dividing_line"><text>\n</text></view>
   <view class='header-title'>
    <view class="product-price-v">
      <text>收货地址</text>
    </view> 
    <view class="ensure">
      <view wx:if="{{userName ==''}}">
        <open-data type="userNickName" lang="zh_CN"></open-data> {{telNumber}} 收
      </view>
      <view wx:if="{{userName !=''}}">
        <text>{{userName}}</text> {{telNumber}} 收
      </view>

       <view style="color:#999;margin-top:10rpx;">
        {{address}} 
        <!-- <view class="chooseAddress" bindtap="getUserAddress">请选择
          <image style="width:12rpx;height:23rpx;" src="data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAAAwAAAAXCAYAAAA/ZK6/AAAAkklEQVQ4jZXU4Q3CIBCG4deLC7iCMzgTXaEjuEK7CivoTv4oWIIc90lCOJLvCVxCuOSc78ANeCMMA15lJhWspd4UZMAOLCqyssrImlpC1u1D1IMQjcAUecBF1wmoqIItOqFFX6iAxNnDEoFUr8LRzz4DP2HwexiGPeCGR2Aa7kEYboEUrkAOV/BUw3C8pQd//BofMLMsPyaca/0AAAAASUVORK5CYII="></image>
        </view>  -->
      </view>
    </view>
  </view>

  <!-- 分隔 -->
  <view class="dividing_line"><text>\n</text></view>
   <view class='header-title'>
    <view class="product-price-v">
      <text>订单金额</text>
    </view> 
    <view class="product-total">
      <text>商品总价</text><text class="product-total-num">{{orderPrice*qty}}元</text>
    </view>
      <view style="padding: 30rpx 0">
      <text>邮费</text><text class="product-total-num">0元</text>
    </view>
  </view>



    <!-- 分隔 -->
    <view class="dividing_line"><text>\n</text></view>
    <view class="bottom-buy-tabBar">
      <view class="bottom-buy-tab" ><button type="primary" class="buy-btn" id="submitForm" form-type='submit' bindtap="payOrder" >支付订单</button></view>
    </view>
</view>
