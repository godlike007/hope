    <block wx:for="{{bannerList}}" wx:key="item">
      <swiper-item>
        <navigator url="{{item.path}}" open-type="{{item.openType}}">
          <image src="{{item.image}}" class="slide-image" data-src="{{item.image}}" style='width:100%;height:300rpx;' />
        </navigator>
      </swiper-item>
    </block>
  </swiper>
