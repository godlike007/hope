<!--pages/test/select3.wxml-->
<view class="section">
  <view class="section__title">多列选择器</view>
  <picker mode="multiSelector" class='asdas' bindchange="bindMultiPickerChange" bindcolumnchange="bindMultiPickerColumnChange" value="{{multiIndex}}" range="{{multiArray}}">
      <view class="picker nav-title">
        {{multiArray[0][multiIndex[0]]}}
        <!-- <input value='{{multiArray[0][multiIndex[0]]}}' placeholder="请选择" readonly="true"></input> -->
      </view>
      <view class="icon"></view>
      <view class=" picker nav-title">
        {{multiArray[1][multiIndex[1]]}}
      </view>
      <view class="icon"></view>
  </picker>
</view>

