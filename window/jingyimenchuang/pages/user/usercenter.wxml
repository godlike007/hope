      <view class="th th1">日期</view>
      <view class="th th2">报修详情</view>
      <view class="th th3">状态</view>
    </view>-->
    <view wx:if="{{activityItemList.length  >0 }}">
      <view class="tr" wx:for="{{activityItemList}}" wx:for-item="item" data-cid='{{item.id}}' bindtap='redirect'>
        <view class="td td1">{{item.name}}</view>
        <view class="td td2" wx:if="{{item.status == 1}}">进行中</view>
        <view class="td td2" wx:if="{{item.status == 3}}">团满</view>
        <view class="td td2" wx:if="{{item.status != 1 && item.status != 3}}">结束</view>
      </view>
    </view>
    <view wx:if="{{activityItemList.length ==0 }}">
        <view class="td td5">你还没有拼团哦~</view>
    </view>
  </view>
