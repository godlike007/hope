/* pages/baoxiu/baoxiu_create.wxss */
page{
  background-color: #f4f4f4;
}
.herder-title{
  font-size: 30rpx !important;
  line-height: 83rpx;
  position: relative;
  margin: -58rpx 15rpx 0;
  border-radius: 11.5rpx;
  background-color: #ffffff;
  padding: 25rpx 30rpx;
}
.header_pic{
  width: 100%;
  height: 300rpx;
}
.txtbtn{
  margin-bottom: -10rpx;
  display: inline-block;
}
.line-hight{
  line-height: 80rpx;
  font-size: 28rpx !important;
  width: 80%;
  margin-left: 10%;
  border-bottom: 2rpx solid #d1d1d1;
}
.submitBtn{
  width: 80%;
  line-height: 75rpx;
  margin-top: 42rpx;
  background-color: #3c3c3c !important;
  color:#ddc977 !important;
  font-size: 25rpx;
}
.button-hover{
   background-color: #000000;
}
.baoxiu-content{
  background-color: #ffffff;
  margin: 15rpx 15rpx;
  border-radius: 11.5rpx;
  box-shadow: 0 0 0.15rem rgba(0,0,0,.12);
  padding: 20rpx 30rpx;
  line-height: 90rpx;
  padding-left: 40rpx;
  font-size: 31rpx;
}
.page_body{
  background-color: #f4f4f4;
  padding-bottom: 3rpx;
}
.textarea{
  height:187rpx !important;
}
.userinfo {  
  position: relative;  
  width: 750rpx;  
  height: 320rpx;  
  color: #fff;  
  display: flex;  
  flex-direction: column;  
  align-items: center;  
} 
.userinfo-avatar {  
  overflow:hidden;  
  display: block;  
  width: 140rpx;  
  height: 140rpx;  
  margin: 20rpx;  
  margin-top: 50rpx;  
  border-radius: 50%;  
  border: 2px solid #fff;  
  box-shadow: 3px 3px 10px rgba(0, 0, 0, 0.2);  
} 
.userinfo{  
  font-size: 14px;  
  background-color: #2FABEE;
}
.nav-all-orde{
  width: 130rpx;
  float: right;
  background-color: #ffffff;
}
