.kefu{
    display: block;
    width: 105rpx;
    height: 105rpx;
    background-image: url('data:image/png;base64,');
    background-size: 100%;
    position: fixed;
    z-index: 100;
    bottom: 20%;
    right: 0;
    margin-right: 5rpx;
    border-radius: 45rpx;
    background-color: transparent;
    border: none;
}
.kefu::after{
  border: none;
}
