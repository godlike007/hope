/* pages/baoxiu/baoxiu_list.wxss */
.history-table-wrap{
 position: relative;
 width: 100%;
 height: 578rpx;
 overflow-y: scroll;
 overflow-x: hidden;
}
/* 表格代码  */
.table{
 border:1px solid #dadada;
 border-right:0;
 border-bottom: 0;
 /*width: 98%;
 margin-left: 1%;*/
}
.tr{
 width:100%;
 display: flex;
 justify-content: space-between;
}
.th,.td{
 padding: 10px;
 border-bottom: 1px solid #dadada;
 border-right: 1px solid #dadada;
 text-align: center;
 width: 100%;
}
.th1,.th3,.td1,.td3{
 width: 30%;
}
.th1,.td5{
 width: 100%;
}
.th{
 font-weight: 800;
 background-color: #b66242;
 font-size: 28rpx;
 color: #330e0e;
}
.td{
 font-size: 20rpx;
 color: #ec9480;
}
.header_pic{
  width: 100%;
  height: 300rpx;
}
.submitBtn{
  width: 80%;
  line-height: 75rpx;
  margin-top: 42rpx;
  background-color: #3c3c3c !important;
  color:#ddc977 !important;
  font-size: 25rpx;
}
.header-title{
  background-color: #dcdcdc;
  /*width: 98%;
  margin-left: 1%;
  margin-bottom: 7rpx;*/
  text-align: center;
  line-height: 63rpx;
  font-size: 28rpx;
}