/* pages/baoxiu/baoxiu_list.wxss */
.history-table-wrap{
 position: relative;
 width: 100%;
 /*height: 578rpx;*/
 overflow-y: scroll;
 overflow-x: hidden;
}
/* 表格代码  */
.table{
 border:1px solid #dadada;
 border-right:0;
 border-bottom: 0;
 /*width: 98%;
 margin-left: 1%;*/
}
.tr{
 width:100%;
 display: flex;
 justify-content: space-between;
}
.th,.td{
 padding: 3px;
 border-bottom: 1px solid #dadada;
 /*border-right: 1px solid #dadada;
 text-align: center;*/
 width: 100%;
}
.th1,.th3,.td1,.td3{
 width: 50%;
}
.th1,.td5{
 width: 100%;
}
.th{
 font-weight: 800;
 background-color: #b66242;
 font-size: 28rpx;
 color: #330e0e;
}
.td{
 font-size: 28rpx;
 color: #241c1a;
}
.td.txt{
  padding-top: 14rpx;
}
.header_pic{
  width: 100%;
  height: 300rpx;
}
.submitBtn{
  width: 80%;
  line-height: 75rpx;
  margin-top: 42rpx;
  background-color: #3c3c3c !important;
  color:#ddc977 !important;
  font-size: 25rpx;
}
.header-title{
  background-color: #E0433E;
  /*width: 98%;
  margin-left: 1%;
  margin-bottom: 7rpx;*/
  text-align: center;
  line-height: 63rpx;
  font-size: 28rpx;
  color: #fff
}
.td_pic{
  width: 100%;
  height: 278rpx;
}
.td_pin{
  padding-top: 14rpx;
}
.goGroupBtn{
  background-position: 0rpx 0rpx;
  background-repeat: no-repeat;
  background-image: url("https://jtljia.oss-cn-hangzhou.aliyuncs.com/jia-web/2019/12/12/ef68423658b84d689ba5c0f2f6aa4553.jpg");
  width: 143rpx;
  line-height: 49rpx;
  color: #ffffff;
  font-size: 28rpx;
}