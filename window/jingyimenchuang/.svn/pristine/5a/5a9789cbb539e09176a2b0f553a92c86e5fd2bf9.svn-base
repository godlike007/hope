    duration="{{duration}}"
    circular="{{circular}}"
    style='width:690rpx;height:360rpx;margin: 0 auto;'
  >
    <block wx:for="{{bannerList}}" wx:key="item">
      <swiper-item>
        <navigator url="{{item.path}}" open-type="{{item.openType}}">
