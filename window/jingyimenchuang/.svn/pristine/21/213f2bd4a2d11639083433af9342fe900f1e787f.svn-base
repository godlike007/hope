  margin-bottom: 0;
  padding-bottom: 0rpx;
}
.brand-block .door-brand .brand-name-desc{
  padding: 0;
  height: 60rpx;
  margin-top: -75rpx;
  width: 99%;
  background: linear-gradient(rgba(0,0,0,.2),rgba(0,0,0,.8));
  position: relative;
  text-align: center;
  line-height: 60rpx;
  color: #fff;
  
}
.commomn_image {
  position: relative;
  border-radius: 20rpx;
