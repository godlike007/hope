  background: url('http://img.xianrenn.com/hmc-web/index_header_back.png');
  background-repeat: no-repeat;
  background-size: 100% 280rpx;
}
.bjtitle{
    font-size: 20px;
    font-weight: bold;
    padding:10px 0;
}
.phoneTxt{
  border-top: 1px solid #ccc;
  border-bottom: 1px solid #ccc;
  line-height: 40px;
  height: 40px;
}
.floor{
   text-align: center;
   font-size: 18px;
}
.submitBtn{
  width: 95%;
  margin-top:2px;
